<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="sTitleOfTextBox_TextBlock" TargetType="TextBlock">

        <Setter Property="FontFamily" Value="{StaticResource fTitleOfTextBlock}" />
        <Setter Property="FontSize" Value="18" />
        <Setter Property="Foreground" Value="{StaticResource cBlack}" />
        <Setter Property="Margin" Value="30 10" />
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="VerticalAlignment" Value="Center" />

    </Style>
</ResourceDictionary>